{
  "name": "sto-helpers",
  "description": "An assortment of helper functions for sto-scheduler and sto-areas",
  "version": "4.2.1",
  "repository": "hawkrives/sto-helpers",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build": "6to5 --optional selfContained src --out-dir lib && node ./bin/make_index.js",
    "clean": "rm -rf ./index.js lib/",
    "push": "./bin/publish.sh",
    "mocha": "mocha",
    "jscs": "jscs ./src ./test",
    "test": "npm run mocha && npm run jscs",
    "watch-tests": "mocha --watch"
  },
  "dependencies": {
    "6to5-runtime": "^3.0.16",
    "combinations-generator": "^1.0.1",
    "lodash": "^3.0.0",
    "sto-course-related-data": "^3.0.3"
  },
  "devDependencies": {
    "6to5": "^3.0.16",
    "chai": "^1.10.0",
    "jscs": "^1.10.0",
    "jscs-jsdoc": "^0.4.4",
    "mocha": "^2.1.0"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
