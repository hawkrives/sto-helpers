{
  "name": "sto-helpers",
  "description": "An assortment of helper functions for sto-scheduler and sto-areas",
  "version": "4.2.8",
  "repository": "hawkrives/sto-helpers",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build": "babel --optional runtime src --out-dir lib && node ./bin/make_index.js",
    "clean": "rm -rf ./index.js lib/",
    "push": "./bin/publish.sh",
    "mocha": "mocha",
    "jscs": "jscs ./src ./test",
    "test": "npm run mocha && npm run jscs",
    "watch-tests": "mocha --watch"
  },
  "dependencies": {
    "babel-runtime": "^4.7.16",
    "combinations-generator": "^1.0.1",
    "lodash": "^3.6.0",
    "sto-course-related-data": "^3.0.6"
  },
  "devDependencies": {
    "babel": "^4.7.16",
    "chai": "^2.1.2",
    "jscs": "^1.11.3",
    "jscs-jsdoc": "^0.4.5",
    "mocha": "^2.2.1"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
