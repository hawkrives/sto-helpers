{
  "name": "sto-helpers",
  "description": "An assortment of helper functions for sto-scheduler and sto-areas",
  "version": "2.1.0",
  "repository": "hawkrives/sto-helpers",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "scripts": {
    "prepublish": "6to5 src --out-dir lib",
    "postpublish": "npm run clean",
    "clean": "rm -rf lib/ dist/",
    "deep-clean": "rm -rf lib/ node_modules/",
    "push": "./bin/publish.sh",
    "test-function": "mocha",
    "test-style": "jscs ./src ./test",
    "test": "npm run test-function && npm run test-style",
    "watch-tests": "nodemon --watch test/ --exec 'npm run test-function'"
  },
  "dependencies": {
    "lodash": "^2.4.1",
    "sto-course-related-data": "^3.0.2"
  },
  "devDependencies": {
    "6to5": "^2.9.3",
    "chai": "^1.10.0",
    "jscs": "^1.9.0",
    "jscs-jsdoc": "^0.4.0",
    "mocha": "^2.1.0",
    "nodemon": "^1.2.1"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
